generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Carrera {
  id                 String @id @default(dbgenerated("uuid('v4')"))
  name               String
  C_kilometers       Float
  adiccional_details String
  fecha              String
  hora               String
}

model Corredor {
  id     String @id @default(dbgenerated("uuid('v4')"))
  name   String
  peso   Float
  altura String
  edad   Int
}

model Follow {
  id               String @id @default(dbgenerated("uuid('v4')"))
  id_carrera       String
  tiempo_llegada   Int
  orden_de_llegada Int
  pulso_inicial    Int
  pulso_final      Int
}
